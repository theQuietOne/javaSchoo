class FirstProgram
{
	public static void main(String[]args)
	{
		int num1=7, num2=4;
		String var1;
		var1="Steeeeeeeeve!!!!!";
		String var2;
		var2="My name is ";
		System.out.println("Hello, World!");
		System.out.println("This is the second line!");
		System.out.println(var2+var1);
		int num3=num1+num2;
		System.out.print(num1+" + "+num2+" = "+num3);
	}
}
