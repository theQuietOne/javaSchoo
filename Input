class Input
{
	public static void main(String[]args)
	{

	EasyReader in = new EasyReader();
	int num1, num2, num3;
	System.out.println("Enter 2 numbers to multiply them together!");
	System.out.println("Type in a number");
	num1=in.readInt();
	System.out.println("Type in another number");
	num2=in.readInt();
	num3=num1*num2;
	System.out.println(num1+" times "+num2+" equals "+num3);
	}
}
