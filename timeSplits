class timeSplits
{
	public static void main(String[]args)
	{
		//enter numbers
		EasyReader in = new EasyReader();
		EasyWriter out = new EasyWriter("timeSplits");
		String prime, segundo, trece, fourer, fiv;
		System.out.println("Enter up to 5 numbers in ascending order using the hh:mm:ss format; I'll find the time elapsed between them");
		prime = in.readLine();
		segundo = in.readLine();
		System.out.println("If you're done entering numbers, enter 00:00:00");
		trece = in.readLine();
		if (!trece.equals("00:00:00"))
		{
			fourer = in.readLine();
			if (!fourer.equals("00:00:00"))
			fiv = in.readLine();
			else
			fiv = "nah";
		}
		else
		{
			trece = "nah";
			fourer = "nah";
			fiv = "nah";
		}
		//convert to times
		int hours, minutes, seconds;
		int onesec, twosec, threesec, foursec, fivesec;
		String conversion;
		conversion = prime.substring(0,2);
		hours = Integer.parseInt(conversion);
		conversion = prime.substring(3,5);
		minutes = Integer.parseInt(conversion);
		conversion = prime.substring(6,8);
		seconds = Integer.parseInt(conversion);
		onesec = seconds + (minutes*60) + (hours*3600)
	}
}
